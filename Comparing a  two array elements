#include<iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int a[n];
    int b[n];
    int flag=0;
    int ans =0;
  for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }  
    for(int i=0;i<n;i++)
    {
        cin>>b[i];
    }
    for(int i=0;i<n;i++)
    {   if(a[i]==b[i])
            ans++;
            flag=1;
    }
if(flag==1)
 cout<<ans<<endl;
  else
cout<<0;

return 0;
}
