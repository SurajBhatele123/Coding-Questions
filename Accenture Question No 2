#include<iostream>
using namespace std;
int main()
{
    int n,sum=0;
    cout<<"Enter the size of array "<<endl;
    cin>>n;
    int a[n];
    char b[n];
    cout<<"Enter the values of a array "<<endl;
    for(int i=0;i<n;i++){
        cin>>a[i]>>b[i];
    }
    for(int i=0;i<n;i++)
    {
        if(b[i] == 'P')
          sum+=a[i];
        else
          sum-=a[i];
    }
cout<<"output is "<<sum*100<<endl;
return 0;
}

