#include<iostream>
#include<bits/stdc++.h>
using namespace std;

string charremove(string str ,char ch){
    int length=str.length();
     string s="";
    for(int i=0;i<length;i++){
         char c=str[i];
         if(c==ch)
            continue;
        else
           s=s+c;
           
    }
    return s;
}
int main(){
    string str;
    cin>>str;
    char ch;
    cin>>ch;
    cout<<charremove(str,ch);
    return 0;
}
